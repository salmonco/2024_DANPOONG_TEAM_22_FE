{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "@components/*": ["./src/components/*"],
      "@constants/*": ["./src/libs/constants/*"],
      "@apis/*": ["./src/libs/apis/*"],
      "@utils/*": ["./src/libs/utils/*"],
      "@hooks/*": ["./src/libs/hooks/*"],
      "@api/*": ["./src/api/*"],
      "@stackNav/*": ["./src/nav/stackNav/*"],
      "@tabNav/*": ["./src/nav/tabNav/*"],
      "@screens/*": ["./src/screens/*"],
      "@assets/*": ["./assets/*"],
      "@types/*": ["./src/types/*"]
    }
  }
}
